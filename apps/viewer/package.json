{
  "name": "@grx/viewer",
  "version": "4.1.8",
  "description": "GPU-Accelerated Web Based EDA Manufacturing Artwork Viewer",
  "main": "./out/electron/main/index.js",
  "author": "Hunter Creery",
  "homepage": "https://grx.electronicloud.app",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit -p tsconfig.web.json --composite false --watch",
    "start": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev:web": "vite",
    "dev:desktop": "electron-vite dev",
    "build:web": "vite build",
    "build:electron": "electron-vite build",
    "build:icons": "electron-icon-builder --input=resources/icon.png --output=resources --flatten",
    "build:win": "npm run build:electron && electron-builder --win --config",
    "build:mac": "npm run build:electron && electron-builder --mac --config",
    "build:linux": "npm run build:electron && electron-builder --linux --config",
    "build:desktop": "npm run build:electron && electron-builder --win --mac --linux --config",
    "dev": "npm run dev:web",
    "build": "npm run build:web"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@grx/engine": "workspace:*",
    "@mantine/carousel": "^7.13.0",
    "@mantine/code-highlight": "^7.13.0",
    "@mantine/core": "^7.13.0",
    "@mantine/dropzone": "^7.13.0",
    "@mantine/form": "^7.13.0",
    "@mantine/hooks": "^7.13.0",
    "@mantine/modals": "^7.13.0",
    "@mantine/notifications": "^7.13.0",
    "@mantine/nprogress": "^7.13.0",
    "@mantine/spotlight": "^7.13.0",
    "@mantine/store": "^8.3.15",
    "@react-hook/resize-observer": "^2.0.2",
    "@react-spring/web": "^10.0.3",
    "@tabler/icons-react": "^3.19.0",
    "@types/react": "^19.2.14",
    "@use-gesture/react": "^10.3.1",
    "chroma-js": "^3.1.1",
    "comlink": "^4.4.2",
    "gl-matrix": "^3.4.4",
    "mantine-contextmenu": "^7.12.2",
    "re-resizable": "^6.10.1",
    "react": "^19.2.0",
    "typescript-event-target": "^1.1.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron-toolkit/utils": "^4.0.0",
    "@electron/notarize": "^3.1.0",
    "@grx/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chroma-js": "^3.1.1",
    "@types/earcut": "^3.0.0",
    "@types/ndarray": "^1.0.14",
    "@types/react-color": "^3.0.13",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "electron-icon-builder": "^2.0.1",
    "electron-updater": "^6.6.2",
    "electron-vite": "^5.0.0",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.1.1",
    "react-dom": "^19.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-comlink": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
